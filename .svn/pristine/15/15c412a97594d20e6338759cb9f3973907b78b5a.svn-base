package com.wk.ssp.mvc.ipush.es.vo;

import java.util.List;

/**
 *
 * 针对创意查询的信息
 *
 */
public class CreativeMessageVO {

    private int w; // 创意的宽
    private int h; // 创意的高
    private List<Integer> item_types; // 广告类型。1：信息流组图，2：信息流小图，3：信息流大图
    private int len; // 主标题最大长度
    private int sub_len; // 副标题最大长度

    /**
     * @return the w
     */
    public int getW() {
        return w;
    }

    /**
     * @param w the w to set
     */
    public void setW(int w) {
        this.w = w;
    }

    /**
     * @return the h
     */
    public int getH() {
        return h;
    }

    /**
     * @param h the h to set
     */
    public void setH(int h) {
        this.h = h;
    }

    /**
     * @return the item_types
     */
    public List<Integer> getItem_types() {
        return item_types;
    }

    /**
     * @param item_types the item_types to set
     */
    public void setItem_types(List<Integer> item_types) {
        this.item_types = item_types;
    }

    /**
     * @return the len
     */
    public int getLen() {
        return len;
    }

    /**
     * @param len the len to set
     */
    public void setLen(int len) {
        this.len = len;
    }

    /**
     * @return the sub_len
     */
    public int getSub_len() {
        return sub_len;
    }

    /**
     * @param sub_len the sub_len to set
     */
    public void setSub_len(int sub_len) {
        this.sub_len = sub_len;
    }

}
