public class LogUtils {
	
	 /**拼接日志字段
	 * @param key
	 * @param value
	 * @return
	 */
	public static String cat(String key, String value){
    	StringBuilder sb= new StringBuilder();
    	sb.append("[").append(key).append(":").append(value).append("]");
    	return sb.toString();
	  }

}
